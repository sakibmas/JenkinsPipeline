pipeline {
    agent {
        label params.Agent
    }
    stages {
        stage('Upload') {
            steps {
                withCredentials([[$class: 'AmazonWebServicesCredentialsBinding', credentialsId: params.AwsCredentialsId, accessKeyVariable: 'AWS_ACCESS_KEY_ID', secretKeyVariable: 'AWS_SECRET_ACCESS_KEY']]) {
                    powershell '''aws s3 ls'''
                }
            }
        }
    }
}
